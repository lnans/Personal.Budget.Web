{
  "name": "personal.budget.web",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "test": "vitest --watch false",
    "test:cc": "vitest --watch false --coverage",
    "test:ui": "vitest --ui",
    "build": "tsc && vite build",
    "lint": "eslint --ext .js,.ts,.tsx ./src --ignore-path .gitignore",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "prettier --list-different .",
    "format": "prettier --write .",
    "validate": "npm-run-all --parallel lint check-types check-format",
    "preview": "vite preview"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.1.1",
    "@emotion/react": "^11.11.1",
    "@iconify/react": "^4.1.0",
    "@mantine/core": "^6.0.13",
    "@mantine/hooks": "^6.0.13",
    "@mantine/notifications": "^6.0.13",
    "@tanstack/react-query": "^4.29.14",
    "@tanstack/react-query-devtools": "^4.29.14",
    "@vitest/coverage-v8": "^0.32.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "i18next": "^23.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-i18next": "^13.0.0",
    "react-router-dom": "^6.13.0"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.32.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-testing-library": "^5.11.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.1"
  },
  "packageManager": "yarn@3.6.0",
  "msw": {
    "workerDirectory": "public"
  }
}
