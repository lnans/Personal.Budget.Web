{
  "editor.rulers": [130],
  "files.eol": "\n",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "i18n-ally.dirStructure": "file",
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.localesPaths": ["packages/life-app-front/public/locales"],
  "i18n-ally.keystyle": "flat",
  "i18n-ally.usage.derivedKeyRules": ["{key}_one", "{key}_other"],
  "i18n-ally.includeSubfolders": true,
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "liveServer.settings.root": "/dist",
  "eslint.workingDirectories": ["packages/life-app-front"],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/bin": true,
    "**/obj": true
  }
}
